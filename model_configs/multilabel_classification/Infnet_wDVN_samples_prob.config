{
    "dataset_reader": {
        "type": "arff",
        "num_labels": 159
    },
    "model": {
        "type": "multi-label-classification",
        "initializer": {
            "regexes": [
                [
                    "score_nn.*",
                    {
                        "type": "pretrained",
                        "weights_file_path": "wandb/DVN/4mepc65o/dvn.th"
                    }
                ],
                [
                    ".*feedforward._linear_layers.*weight",
                    {
                        "gain": 1.6666666666666667,
                        "type": "xavier_uniform"
                    }
                ],
                [
                    ".*linear_layers.*bias",
                    {
                        "type": "zero"
                    }
                ]
            ]
        },
        "loss_fn": {
            "type": "multi-label-dvn-bce",
            "reduction": "mean"
        },
        "oracle_value_function": {
            "type": "per-instance-f1"
        },
        "sampler": {
            "type": "infnet-multi-sample-prob",
            "inference_nn": {
                "type": "multi-label-classification",
                "feature_network": {
                    "activations": [
                        "tanh",
                        "tanh"
                    ],
                    "dropout": [
                        0.5,
                        0
                    ],
                    "hidden_dims": 150,
                    "input_dim": 1836,
                    "num_layers": 2
                },
                "label_embeddings": {
                    "embedding_dim": 150,
                    "vocab_namespace": "labels"
                }
            },
            "loss_fn": {
                "type": "combination-loss",
                "constituent_losses": [
                    {
                        "reduction": "none",
                        "type": "multi-label-dvn-score"
                    },
                    {
                        "reduction": "none",
                        "type": "multi-label-bce"
                    }
                ],
                "loss_weights": [
                    0.8934020442650806,
                    1
                ],
                "reduction": "mean"
            },
            "loss_fn_sample": {
                "reduction": "none",
                "type": "multi-label-bce-unreduced"
            },
            "loss_fn_for_grad": {
                "reduction": "none",
                "type": "multi-label-dvn-score"
            },
            "sample_loss_weight": 0.5,
            "num_samples": 5,
            "optimizer": {
                "type": "adam",
                "lr": 0.0025340631422498053,
                "weight_decay": 0.0001
            },
            "stopping_criteria": 5
        },
        "score_nn": {
            "type": "multi-label-classification",
            "global_score": {
                "type": "multi-label-feedforward",
                "feedforward": {
                    "activations": "tanh",
                    "hidden_dims": 200,
                    "input_dim": 159,
                    "num_layers": 1
                }
            },
            "task_nn": {
                "type": "multi-label-classification",
                "feature_network": {
                    "activations": [
                        "tanh",
                        "tanh",
                        "tanh"
                    ],
                    "dropout": [
                        0.3,
                        0.3,
                        0
                    ],
                    "hidden_dims": 150,
                    "input_dim": 1836,
                    "num_layers": 3
                },
                "label_embeddings": {
                    "embedding_dim": 150,
                    "vocab_namespace": "labels"
                }
            }
        }
    },
    "train_data_path": "./data//bibtex_original/train.arff",
    "validation_data_path": "./data//bibtex_original/val.arff",
    "test_data_path": "./data//bibtex_original/test.arff",
    "trainer": {
        "callbacks": [
            "track_epoch_callback",
            {
                "model_outputs_to_log": [
                    "y_hat_extra"
                ],
                "tensorboard_writer": {
                    "should_log_learning_rate": true
                },
                "type": "tensorboard-custom"
            },
        ],
        "checkpointer": {
            "num_serialized_models_to_keep": 1
        },
        "cuda_device": 0,
        "learning_rate_scheduler": {
            "type": "reduce_on_plateau",
            "factor": 0.5,
            "mode": "max",
            "patience": 5,
            "verbose": true
        },
        "num_epochs": 300,
        "optimizer": {
            "type": "adam",
            "lr": 0.10440715821874887,
            "weight_decay": 5.029804507926999e-05
        },
        "patience": 20,
        "validation_metric": "+fixed_f1"
    },
    "data_loader": {
        "batch_size": 32,
        "shuffle": true
    },
    "evaluate_on_test": true,
    "validation_dataset_reader": {
        "type": "arff",
        "num_labels": 159
    }
}